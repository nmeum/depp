<section id="blob">
	{{ template "breadcrumb.tmpl" .CurrentFile }}

	{{- $blob := .GetBlob (.CurrentFile.Path) -}}
	<pre class="blob">
{{- $lines := (getLines $blob) -}}
{{- range $i, $line := $lines }}
<a id="L{{ $i }}" href="#L{{ $i }}">{{ getPadding (len $lines) $i }}{{ $i }}</a>{{ . }}
{{- end }}
	</pre>
</section>
